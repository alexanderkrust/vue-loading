{
  "name": "vue-loading-workspace",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "build:components": "cd packages/vue-loading && pnpm build",
    "build:nuxt-module": "cd packages/nuxt && pnpm build",
    "build:package": "pnpm build:components && pnpm build:nuxt-module",
    "test:package": "cd test && pnpm test:unit",
    "build:docs": "cd docs && pnpm build",
    "docs:dev": "concurrently --kill-others \"cd packages/vue-loading && pnpm dev\" \"cd docs && pnpm dev\"",
    "test:components:watch": "concurrently --kill-others \"cd packages/vue-loading && pnpm dev\" \"pnpm test:package\"",
    "storybook": "concurrently --kill-others \"cd packages/vue-loading && pnpm dev\" \"storybook dev -p 6006\"",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@alexanderkrust/vue-loading": "workspace:*",
    "@chromatic-com/storybook": "^1.6.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@storybook/vue3": "^8.1.11",
    "@storybook/vue3-vite": "^8.1.11",
    "eslint": "^9.6.0",
    "husky": "^9.0.11",
    "storybook": "^8.1.11",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@semantic-release/github": "^10.0.7",
    "@semantic-release/npm": "^12.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.4.0",
    "tailwindcss": "^3.4.4"
  }
}
