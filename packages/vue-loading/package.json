{
  "name": "@alexanderkrust/vue-loading",
  "version": "1.1.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "repository": "git://github.com/alexanderkrust/vue-loading.git",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./index.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./nuxt": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "./LICENSE",
    "./dist",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf ./dist && vite build && vue-tsc --declaration --emitDeclarationOnly",
    "dev": "nodemon --watch src --ext vue,css,ts --exec \"pnpm build\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": ">= 3.4.27"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.12.2",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.38",
    "radix-vue": "^1.8.3",
    "rimraf": "^5.0.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vue-tsc": "^2.0.19"
  }
}
